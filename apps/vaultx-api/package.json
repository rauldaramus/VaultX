{
  "name": "@vaultx-monorepo/vaultx-api",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-prometheus": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.54.0",
    "@opentelemetry/instrumentation-ioredis": "^0.54.0",
    "@opentelemetry/instrumentation-mongoose": "^0.53.0",
    "@opentelemetry/resources": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.13.3",
    "mongoose": "^8.9.2",
    "mongosh": "^2.5.9",
    "pino": "^9.13.1",
    "reflect-metadata": "^0.2.2"
  },
  "nx": {
    "targets": {
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@vaultx-monorepo/vaultx-api:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@vaultx-monorepo/vaultx-api:build:development"
          },
          "production": {
            "buildTarget": "@vaultx-monorepo/vaultx-api:build:production"
          }
        }
      }
    }
  }
}
